{
  "name": "serverless-full-stack-engineering",
  "version": "1.0.0",
  "private": false,
  "dependencies": {
    "lunr": "^2.3.6",
    "antd": "^3.13.4",
    "redux": "^4.0.1",
    "nanoid": "^2.0.1",
    "react": "^16.8.2",
    "redux-thunk": "^2.3.0",
    "react-dom": "^16.8.2",
    "react-quill": "^1.3.3",
    "react-redux": "^6.0.1",
    "react-scripts": "2.1.5",
    "amazon-cognito-identity-js": "^3.0.7",
    "react-lazy-load-image-component": "^1.3.2"
  },
  "scripts": {
    "eject": "react-scripts eject",
    "test": "react-app-rewired test",
    "remove": "cd backend && serverless remove",
    "start": "env-cmd ./.dev.env react-app-rewired start",
    "build": "env-cmd ./.dev.env react-app-rewired build",
    "deploy": "cd backend && serverless deploy -v --stage",
    "invoke": "cd backend && serverless invoke --function",
    "deploy-web": "yarn run build && cd backend && serverless client deploy --no-confirm --no-cors-change"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "npm": "^6.8.0",
    "env-cmd": "^8.0.2",
    "webpack": "4.28.3",
    "install": "^0.12.2",
    "standard": "^12.0.1",
    "aws-sdk": "^2.419.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "imports-loader": "^0.8.0",
    "serverless-finch": "^2.4.2",
    "babel-preset-env": "^1.7.0",
    "react-app-rewired": "^2.1.0",
    "serverless-webpack": "^5.2.0",
    "serverless-s3-remover": "^0.6.0",
    "webpack-node-externals": "^1.7.2",
    "serverless-stack-output": "^0.2.3",
    "serverless-dynamodb-autoscaling": "^0.6.2",
    "serverless-single-page-app-plugin": "^1.0.2"
  }
}
