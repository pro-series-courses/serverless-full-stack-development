{
  "name": "serverless-full-stack-engineering",
  "version": "1.0.0",
  "private": false,
  "dependencies": {
    "amazon-cognito-identity-js": "^3.0.7",
    "antd": "^3.13.4",
    "aws-sdk": "^2.419.0",
    "lunr": "^2.3.6",
    "nanoid": "^2.0.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-lazy-load-image-component": "^1.3.2",
    "react-quill": "^1.3.3",
    "react-redux": "7.0.2",
    "react-scripts": "2.1.5",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "eject": "react-scripts eject",
    "test": "env-cmd ./.dev.env react-app-rewired test",
    "start": "env-cmd ./.dev.env react-app-rewired start",
    "build": "env-cmd ./.dev.env react-app-rewired build",
    "deploy": "cd backend && serverless deploy -v",
    "remove": "cd backend && serverless remove -v",
    "invoke": "cd backend && serverless invoke --function",
    "deploy-web": "yarn run build && cd backend && serverless client deploy --no-confirm --no-cors-change"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "moduleNameMapper": {
      "^actions(.*)$": "<rootDir>/src/actions$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^lib(.*)$": "<rootDir>/src/lib$1",
      "^reducers(.*)$": "<rootDir>/src/reducers$1",
      "^media(.*)$": "<rootDir>/src/media$1"
    },
    "setupFiles": [
      "jest-prop-type-error"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "env-cmd": "^8.0.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "imports-loader": "^0.8.0",
    "install": "^0.12.2",
    "jest-enzyme": "^7.0.2",
    "jest-prop-type-error": "^1.1.0",
    "jsdom": "^15.0.0",
    "moxios": "^0.4.0",
    "npm": "^6.8.0",
    "react-app-rewired": "^2.1.0",
    "redux-mock-store": "^1.5.3",
    "serverless-dynamodb-autoscaling": "^0.6.2",
    "serverless-finch": "^2.4.2",
    "serverless-s3-remover": "^0.6.0",
    "serverless-single-page-app-plugin": "^1.0.2",
    "serverless-stack-output": "^0.2.3",
    "serverless-webpack": "^5.2.0",
    "shelljs": "^0.8.3",
    "standard": "^12.0.1",
    "webpack": "4.28.3",
    "webpack-node-externals": "^1.7.2"
  }
}
