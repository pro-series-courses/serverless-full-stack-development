{
  "name": "serverless-full-stack-development",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "antd": "^3.13.4",
    "apollo-server-lambda": "^2.4.4",
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.4",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-scripts": "2.1.5"
  },
  "scripts": {
    "start": "env-cmd ./.dev.env react-scripts start",
    "build": "env-cmd ./.pro.env react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "cd backend && serverless deploy -v --stage",
    "remove": "cd backend && serverless remove -v --stage",
    "invoke": "cd backend && serverless invoke --function"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "env-cmd": "^8.0.2",
    "install": "^0.12.2",
    "npm": "^6.8.0",
    "serverless-stack-output": "^0.2.3",
    "serverless-webpack": "^5.2.0"
  }
}
